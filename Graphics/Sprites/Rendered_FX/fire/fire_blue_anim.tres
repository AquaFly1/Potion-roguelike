[sub_resource type="GDScript" id="GDScript_86hjl"]
script/source = "func hue_shift_image(img: Image, shift: float) -> Image:
	img.lock()
	for y in img.get_height():
		for x in img.get_width():
			var c = img.get_pixel(x,y)
			var hsv = c.to_hsv()
			hsv.x = fmod(hsv.x + shift, 1.0)
			img.set_pixel(x,y, Color.from_hsv(hsv.x, hsv.y, hsv.z, c.a))
	img.unlock()
	return img
"

